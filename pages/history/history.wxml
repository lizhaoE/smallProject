<view class="container">
  <text catchtap="longPress" class="title">历史记录</text>
  <text class="point">长按清除</text>
</view>

<view class="container_from_success"> 
  <view class="container_grip" >
    <view class="image_style" >
      <image class="image" src="{{responseList[0].baike_info.image_url}}">
      </image>
    </view>
  </view>

  <view class="container_grip">
    <view class="text_brief">
      <text class="name">【{{responseList[0].name}}】</text>
      <text class="text">{{responseList[0].baike_info.description}}</text>
    </view>
  </view>

  <view class="container_grip">
   <text class="name">【联想】</text>
    <view class="relation_flower">
      <block wx:for="{{responseList}}" wx:for-item="item"  wx:key="key" >
        <text>【{{item.name}}】</text>
      </block>
   </view>
 </view>

</view>